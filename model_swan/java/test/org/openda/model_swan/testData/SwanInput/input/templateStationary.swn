!*************************HEADING************************

PROJ 'A04bG1' '001'

!********************MODEL INPUT*************************

SET NAUTICAL 
SET LEVEL=0

MODE STATIONARY 

COORDINATES SPHERICAL CCM

CGRID   REGULAR -12. 48. 0. 21. 16. 420 480 CIRCLE 45 0.03 0.6

INPGRID BOTTOM REGULAR -15. 43. 0. 1120 1260  0.025   0.0166  EXC -9999.
READINP BOTTOM 1. 'INP/dcsm_IGB_NES_NF.bot' idla=3 FREE

!-DCSMv6 water level input
!!INPGRID WLEV REGULAR -11.938 48.0822 0.0 167 171 0.125 0.0833 EXC 999999. NONSTAT $TSTART$ 1 HR $TSTOP$
!!INPGRID WLEV REGULAR -12.0 48.0 0. 210 240 0.10 0.06667 EXC 999999. NONSTAT $TSTART$ 1 HR $TSTOP$
!!READINP WLEV 1. SERIES 'INP/WLEV.SER' idla=3 FREE
INPGRID wlev REGULAR -12 48 0.0 420 480    0.05000    0.03333 &
   EXC -99. NONSTAT $TSTART$ 60 MIN $TSTOP$
READINP wlev 1. SERIES 'fews_flow_wlev.inp' idla=3 FREE

!-UKMO wind input
!!INPGRID WIND   REGULAR -12.0 48.0 0. 190 144 0.11 0.11 NONSTAT 20071108.0800  3 HR 20071110.0200
!!INPGRID WIND REGULAR -12.0 48.0 0. 210 240 0.10 0.06667 NONSTAT $TSTART$ 1 HR $TSTOP$
!!READINP WIND 1. SERIES 'INP/WIND.SER' idla=3 FREE 
INPGRID wind REGULAR -12 48 0.0 210 240    0.10000    0.06667 &
   EXC -99. NONSTAT $TSTART$ 60 MIN $TSTOP$
READINP wind 1. SERIES 'fews_wind.inp' idla=3 FREE


!************************************* BOUNDARY CONDITIONS **************************************

!!BOUND NEST 'INP/wam_heleperiode.bnd' OPEN
!!INIT HOTSTART SING 'A03aG1T1.HOT'
!BOUND NEST 'INP/ECMWF.BND' OPEN
BOUND NEST 'SPECTRA.BND' OPEN
$HOTSTART$
$NCRESTART$

!****************************************** PHYSICA *********************************************


GEN3 
WCAP KOM 0.0000236 0.00302 2.0 1.0 1.0
FRIC JONSWAP   cfjon=0.038
BREA WESTH     alpha=0.96   pown=2.5       bref=-1.3963   shfac=500


!************************************ NUMERIEKE PARAMETERS ***************************************

PROP BSBT
NUM ACCUR NONSTAT mxitns=20

!************************************ OUTPUT ***************************************

POINTS 'PZ'    FILE 'INP/ZUNO.PNT'
