BLOCK 'COMPGRID' NOHEAD  'RES/SWAN.mat' LAYOUT 3 &
   XP YP HSIG HSWELL TMM10 DIR DSPR WATLEV &
   OUT $TSTART$ 1 HR

SPECOUT 'P1' SPEC1D ABS 'RES/SPEC_P1.SP1' OUT $TSTART$ 1 HR
SPECOUT 'PZ' SPEC2D ABS 'RES/SPEC_PZ.SP2' OUT $TSTART$ 1 HR 
TABLE   'P1' HEADER     'RES/POINTS.TAB' &
        TIME XP YP DEP HSIG HSWELL TMM10 DIR DSPR WATLEV WIND &
        OUT $TSTART$ 1 HR

TEST 1 0

$STATIONARY$

HOTFILE 'swan-state'

STOP
