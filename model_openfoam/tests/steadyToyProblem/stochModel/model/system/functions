functions
{
residuals
{
    type        residuals;
    functionObjectLibs ("libutilityFunctionObjects.so");
    writeControl    timeStep;
    writeInterval   1;
    fields        (U p_rgh k omega T        );
}
probe_Assimilatie_T
{
type    probes;
libs    ("libsampling.so");
writeControl    timeStep;
writeInterval    1;
fields    (T );
probeLocations
(
        (2.4 3.0 4.5)
        (4.8 0.9 4.5)
        (4.8 3.0 4.5)

    );

}
