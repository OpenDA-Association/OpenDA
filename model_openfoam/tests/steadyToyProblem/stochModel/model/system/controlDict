application         buoyantBoussinesqSimpleFoam;

startFrom           startTime;

startTime           0;//#oda:startTime

stopAt              endTime;

endTime             30;//#oda:endTime

deltaT              1;//1

writeControl        adjustableRunTime;

writeInterval       10;

purgeWrite          1;

writeFormat         ascii;

writePrecision      8;

writeCompression    no;

timeFormat          general;

timePrecision       6;

graphFormat         xmgr;

runTimeModifiable   yes;

adjustTimeStep      no;

maxCo               5;

maxDeltaT           1;

libs ( "libmeshTools.so" "libfiniteVolume.so" ) ;

