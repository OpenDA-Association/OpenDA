FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.06;

boundaryField
{
    wall//".*"
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    cyclic
    {
        type            cyclic;
        value           $internalField;
    }
    CRAC_Return
    {
        type            inletOutlet;
        inletValue      $internalField;// tbd
        value           $internalField;
    }
    CRAC_Supply
    {
        type            turbulentIntensityKineticEnergyInlet;
        value           $internalField;
        intensity       0.1;
    }
}


