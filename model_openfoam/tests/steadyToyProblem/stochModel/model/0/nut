FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 0.001;

boundaryField
{
    wall
    {
        type            nutUSpaldingWallFunction;
        value           $internalField;
    }
    cyclic
    {
        type            cyclic;
        value           $internalField;
    }
    "CRAC.*let"
    {
        type            calculated;
        value           $internalField;
    }
}


