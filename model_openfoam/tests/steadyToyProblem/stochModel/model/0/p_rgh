FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    ".*"
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    cyclic
    {
        type            porousBafflePressure;
        patchType       cyclic;
        jump            uniform 0;
        D               0;
        I               10;//857;
        length          0.01;
        value           uniform 0;
    }
    CRAC_Inlet
    {
        type            totalPressure;
        p0              uniform 0;
        gamma           1.4;
        value           $internalField;
        rho             rhok;
    }
}


