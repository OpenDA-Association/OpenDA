<?xml version="1.0" encoding="UTF-8"?>
<blackBoxModelConfig xmlns="http://www.openda.org"
                     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:schemaLocation="http://www.openda.org http://schemas.openda.org/blackBoxModelConfig.xsd">
    <!-- Specify wrapper configuration for which this model is defined -->
    <wrapperConfig>
        <file>openfoamWrapper.xml</file>
    </wrapperConfig>

    <!-- Specify wrapper configuration for which this model is defined -->
    <aliasValues>
        <alias key="exedir" value="./bin"/>
        <alias key="instanceDir" value="../work"/>
        <alias key="templateDir" value="./model"/>
        <alias key="runid" value="steadyToyProblem"/>
        <alias key="controlDict" value="controlDict"/>
        <alias key="numberOfSubdomains" value="1"/>
        <alias key="foamSolver" value="buoyantBoussinesqSimpleFoam"/>
        <alias key="referenceDateTime" value="2018-04-01T00:00:00Z"/>
    </aliasValues>

    <!-- <timeInfo start="2018-04-01T00:00:00Z" end="2018-04-01T00:00:10Z"/> -->

    <timeInfoExchangeItems start="startTime" end="endTime"/>

    <exchangeItems>
        <vector id="startTime" ioObjectId="controlDict" elementId="oda:startTime"/>
        <vector id="endTime" ioObjectId="controlDict" elementId="oda:endTime"/>
        <vector id="HeatX0Y0" ioObjectId="HEATING" elementId="oda:HeatX0Y0@1"/>
        <vector id="HeatX1Y0" ioObjectId="HEATING" elementId="oda:HeatX1Y0@1"/>
        <vector id="HeatX0Y1" ioObjectId="HEATING" elementId="oda:HeatX0Y1@1"/>
        <vector id="HeatX1Y1" ioObjectId="HEATING" elementId="oda:HeatX1Y1@1"/>
        <vector id="probe_Assimilatie_T#0.T" ioObjectId="ProbesT" elementId="probe_Assimilatie_T#1.T"/>
        <vector id="probe_Assimilatie_T#1.T" ioObjectId="ProbesT" elementId="probe_Assimilatie_T#1.T"/>
        <vector id="probe_Assimilatie_T#2.T" ioObjectId="ProbesT" elementId="probe_Assimilatie_T#2.T"/>
        <vector id="probe_Assimilatie_T#3.T" ioObjectId="ProbesT" elementId="probe_Assimilatie_T#3.T"/>
    </exchangeItems>

<doCleanUp>false</doCleanUp>

</blackBoxModelConfig>

