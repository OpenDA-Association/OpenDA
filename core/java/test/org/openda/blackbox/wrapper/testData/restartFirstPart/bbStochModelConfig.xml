<?xml version="1.0" encoding="UTF-8"?>
<blackBoxStochModel xmlns="http://www.openda.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.openda.org http://schemas.openda.org/blackBoxStochModelConfig.xsd">

    <modelConfig>
        <file>./bbModelConfig.xml</file>
    </modelConfig>

    <vectorSpecification>
        <state>
            <noiseModel>
                <vector id="bc-B"/>
                <armaModel>
                    <stdDev value="40." transformation="ln"/>
                    <armaConstant>0.95</armaConstant>
                </armaModel>
            </noiseModel>
            <noiseModel>
                <vector id="bc-C"/>
                <armaModel>
                    <stdDev value="80." transformation="ln"/>
                    <armaConstant>0.95</armaConstant>
                </armaModel>
            </noiseModel>
            <vector id="state"/>
        </state>
    </vectorSpecification>

</blackBoxStochModel>
