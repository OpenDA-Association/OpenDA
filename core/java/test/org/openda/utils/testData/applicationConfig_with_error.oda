<?xml version="1.0" encoding="UTF-8"?>
<openDaApplication xmlns="http://www.openda.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.openda.org http://www.openda.org/schemas/openda_2.1/openDaApplication.xsd">
	<stochObserver className="nl.deltares.openda.observers.DaStochObserver">
		<workingDirectory>./stochobserver</workingDirectory>
		<configFile>swanStochObsConfig.xml</configFile>
	</stochObserver>
	<!-- a true bakery is not a factory of course -->
	<stochModelBakery className="nl.deltares.openda.models.DaModelFactory">
		<workingDirectory>./swanModel/config</workingDirectory>
		<configFile>openDaStochModel_linux.xml</configFile>
	</stochModelBakery>
	<algorithm className="org.openda.algorithms.Dud">
		<workingDirectory>./algorithm</workingDirectory>
		<configString>dudAlgorithm.xml</configString>
	</algorithm>
	<resultWriter className="org.openda.resultwriters.MatlabResultWriter">
		<workingDirectory>.</workingDirectory>
		<configFile>dud_results.m</configFile>
	</resultWriter>
</openDaApplication>
